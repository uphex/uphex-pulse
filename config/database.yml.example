---
production:
  adapter: postgresql
  username: <%= ENV['POSTGRESQL_USERNAME'] %>
  password: <%= ENV['POSTGRESQL_PASSWORD'] %>
  database: <%= ENV['POSTGRESQL_DATABASE'] %>
  host:     <%= ENV['POSTGRESQL_ADDRESS'] %>


development:
  adapter: postgresql
  username: postgres
  database: uphex-development
  host: localhost

test:
  adapter: postgresql
  username: postgres
  database: uphex-test
  host: localhost
